module tests.trait_relationships @ 1.0.0

/// A simple gene for testing
pub gene SimpleValue {
    has value: String
    has count: Int64 = 0

    fun get_value() -> String {
        return this.value
    }
}

/// Trait for display functionality
pub trait Displayable {
    /// Return string representation
    is display() -> String
}

/// Gene that wraps strings
pub gene StringWrapper {
    has value: String

    fun to_string() -> String {
        return this.value
    }
}

exegesis {
    Tests for trait definitions and relationships.
}
